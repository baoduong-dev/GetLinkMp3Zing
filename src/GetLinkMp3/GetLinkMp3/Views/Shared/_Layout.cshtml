<!DOCTYPE html>
<html>
<head itemscope="" itemtype="http://schema.org/WebPage" prefix="og: http://ogp.me/ns# fb: http://ogp.me/ns/fb# website: http://ogp.me/ns/website#">
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    <meta name="description" content="@ViewBag.Description" />
    <meta name="copyright" content="DivBridge.net" />
    <meta name="author" content="DivBridge.net" />
    <meta name="revisit-after" content="1 days" />
    <meta name="GENERATOR" content="DivBridge.net" />
    <meta itemprop="name" content="@ViewBag.Title" />
    <meta itemprop="url" content="@Request.Url.AbsoluteUri" />
    <meta itemprop="description" content="@ViewBag.Description" />
    <meta itemprop="image" content="@ViewBag.Image" />

    <meta property="og:title" content="@ViewBag.Title" />
    <meta property="og:type" content="website" />
    <meta property="og:url" itemprop="url" content="@Request.Url.AbsoluteUri" />
    <meta property="og:image" content="@ViewBag.Image" />
    <meta property="og:site_name" content="divbridge.net" />
    <meta property="og:description" itemprop="description" content="@ViewBag.Description" />
    <meta property="fb:app_id" content="885711171547202"/>

    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    @Styles.Render("~/Content/css")
    @*@Scripts.Render("~/bundles/modernizr")*@
    <link href="~/Content/images/favicon.png" rel="shortcut icon" type="image/x-icon" />

</head>
<body>
    <nav class="light-blue lighten-1" role="navigation">
        <div class="nav-wrapper container">
            <a id="logo-container" href="#" class="brand-logo">Like Music</a>
            <ul class="right hide-on-med-and-down">
                <li><a class="contact" href="#!">Liên hệ</a></li>
            </ul>
            <ul id="nav-mobile" class="side-nav">
                <li><a class="contact" href="#!">Liên hệ</a></li>
            </ul>
            <a href="#" data-activates="nav-mobile" class="button-collapse"><i class="material-icons">menu</i></a>
        </div>
    </nav>
    @RenderBody()
    <footer class="page-footer orange">
        <div class="footer-copyright">
            <div class="container">
                Copyright &copy; 2016 <a class="orange-text text-lighten-3" href="http://divbrigde.Net">DivBrigde.Net</a>
            </div>
        </div>
    </footer>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/materialize")
    @RenderSection("scripts", required: false)
    <script>
  window.fbAsyncInit = function() {
    FB.init({
      appId      : '885711171547202',
      xfbml      : true,
      version    : 'v2.5'
    });
  };

  (function(d, s, id){
     var js, fjs = d.getElementsByTagName(s)[0];
     if (d.getElementById(id)) {return;}
     js = d.createElement(s); js.id = id;
     js.src = "//connect.facebook.net/en_US/sdk.js";
     fjs.parentNode.insertBefore(js, fjs);
   }(document, 'script', 'facebook-jssdk'));
    </script>
</body>
</html>
