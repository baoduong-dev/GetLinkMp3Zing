<div class="card light-blue lighten-1 hoverable">
    <div class="card-image">
        <img src="@ViewBag.imagessong">
        <span class="card-title">@ViewBag.artist</span>
    </div>
    <div class="card-content white-text">
        <span class="card-title">
            Bài hát: <strong>@ViewBag.name</strong>
        </span>
        <p>Link download 320kbps: <a href="@ViewBag.linkdown320" target="_blank">Tải tại đây</a></p>
        <p>Link download lossless (50/50): <a href="@ViewBag.linkdownloss" target="_blank">Tải tại đây</a></p>
    </div>
</div>
@*<meta http-equiv="refresh" content="0;URL=@ViewBag.linkdown" />*@